<map proto="1.3.5">
<name>bla</name>
<version>1.0.0</version>
<objective>Be the last team alive!</objective>
<authors>
    <author uuid="dff980ed-9f69-491e-b79c-30c21350baf2"/> <!-- DoomRobotBoy -->
</authors>
<contributors>
    <contributor uuid="a4266139-34f3-46c5-a730-f8828376a15a" contribution="Build Help"/> <!-- LArrowHead -->
</contributors>
<broadcasts>
    <tip after="1s">You can capture the flag at the beacon after the entire enemy team is dead.</tip>
</broadcasts>
<teams>
    <team color="blue" max="20" max-overfill="24">Blue Team</team>
    <team color="red" max="20" max-overfill="24">Red Team</team>
</teams>
<kits>
    <kit name="all">
        <potion duration="oo">night vision</potion>
    </kit>
    <kit name="spawn" parents="all">
        <item slot="0" unbreakable="true">stone sword</item>
        <item slot="1" unbreakable="true" enchantment="arrow_infinite:1">bow</item>
        <item slot="28" amount="1">arrow</item>
        <chestplate unbreakable="true">iron chestplate</chestplate>
        <boots unbreakable="true" enchantment="protection_fall:2">iron boots</boots>
        <item slot="2" amount="64">golden carrot</item>
        <health>20</health>
    </kit>
    <kit name="red-kit" parents="spawn">
        <helmet color="AF002A" unbreakable="true">leather helmet</helmet>
    </kit>
    <kit name="blue-kit" parents="spawn">
        <helmet color="6E6EF9" unbreakable="true">leather helmet</helmet>
    </kit>
    <kit name="flag" force="true">
        <potion duration="oo" amplifier="-3">health boost</potion>
    </kit>
    <kit name="reset-health" force="true">
        <potion duration="1" amplifier="0">health boost</potion>
        <potion duration="1" amplifier="10">regeneration</potion>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="red-kit" yaw="-90" filter="blue-respawn">
        <cuboid min="1.5,8,-449.5" max="5.5,8,-447.5"/>
    </spawn>
    <spawn team="blue" kit="blue-kit" yaw="90" filter="yes-flag">
        <cuboid min="1.5,8,-443.5" max="5.5,8,-441.5"/>
    </spawn>
    <default yaw="0" kit="all">
        <point>3.5,53,-445.5</point>
    </default>
</spawns>
<respawn spectate="true">
    <message>
        { translate: "death.respawn.confirmed.waiting.flagDropped" }
    </message>
</respawn>
<filters>
    <filter name="blue-respawn">
        <any>
            <same-team>
                <carrying-flag>flag</carrying-flag>
            </same-team>
            <all>
                <team>red</team>
                <objective>hill</objective>
            </all>
        </any>
    </filter>
</filters>
<regions>
    <rectangle name="map" min="oo,oo" max="-oo,oo"/>
    <apply block="deny-players">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<control-points>
    <control-point id="hill" capture-time="0s" show-progress="true" point="0">
        <capture><region name="map"/></capture>
        <progress><cylinder base="0.5,22,7.5" radius="7" height="4"/></progress>
        <captured><cylinder base="0.5,64,7.5" radius="7" height="5"/></captured>
    </control-point>
</control-points>
<itemremove>
    <item>leather helmet</item>
    <item>golden apple</item>
    <item>iron boots</item>
    <item>iron chestplate</item>
    <item>stone sword</item>
    <item>bow</item>
    <item>arrow</item>
    <item>golden carrot</item>
</itemremove>
<score>
    <limit>200</limit>
    <kills>0</kills>
    <deaths>0</deaths>
</score>
<killreward>
    <item>golden apple</item>
</killreward>
<flags>
    <net id="net" flag="flag">
        <block>-260.5,5,98.5</block>
    </net>
    <post id="post" return-time="0s" respawn-time="7s">
        <block>-260.5,5,98.5</block>
        <block>3.5,20,-475.5</block>
        <block>3.5,20,-415.5</block>
        <block>3.5,20,-475.5</block>
        <block>3.5,20,-415.5</block>
    </post>
    <flag id="flag" name="Flag" shared="true" post="post" net="net" points-rate="1" pickup-kit="flag" drop-kit="reset-health"/>
</flags>
<score>
    <limit>3</limit>
</score>
</map>
