<map proto="1.4.0">
<name>Mantis TE</name>
<version>1.0.0</version>
<objective>Capture the other team's wool to win!</objective>
<authors>
    <author uuid="46552b61-8564-46a8-aa87-5c246b137913"/> <!--  K10s  -->
</authors>
<contributors>
    <contributor contribution="XML" uuid="dff980ed-9f69-491e-b79c-30c21350baf2"/> <!-- DoomyBoy -->
</contributors>
<teams>
    <team id="yellow" color="yellow" max="6">Yellow</team>
    <team id="blue" color="aqua" max="6">Light Blue</team>
</teams>
<spawns>
    <spawn team="yellow" kit="spawn">
        <regions>
            <point yaw="0">1,7,-51</point>
        </regions>
    </spawn>
    <spawn team="blue" kit="spawn">
        <regions>
            <point yaw="180">1,7,53</point>
        </regions>
    </spawn>
    <default>
        <regions yaw="90">
            <cuboid min="46,28,0" max="44,28,2"/>
        </regions>
    </default>
</spawns>
<kits>
    <kit id="spawn" force="true">
        <item slot="0" unbreakable="true">iron sword</item>
        <item slot="1" unbreakable="true">bow</item>
        <item slot="2" unbreakable="true">iron pickaxe</item>
        <item slot="3" unbreakable="true">iron axe</item>
        <item slot="4" amount="64">golden carrot</item>
        <item slot="5" amount="48" damage="1">wood</item>
        <item slot="6" amount="32">arrow</item>
    </kit>
</kits>
<kill-rewards>
    <kill-reward>
        <item amount="8" material="arrow"/>
        <item amount="12" damage="1" material="wood"/>
    </kill-reward>
</kill-rewards>
<filters>
    <not id="no-void">
        <void/>
    </not>
    <team id="only-red">red-team</team>
    <team id="only-blue">blue-team</team>
    <material id="only-gold">gold block</material>
    <not id="not-spawners">
        <all>
            <any>
                <material>mob spawner</material>
                <material>iron plate</material>
            </any>
        </all>
    </not>
    <all id="only-red-cobweb">
        <team>red-team</team>
        <material>web</material>
    </all>
    <all id="only-blue-cobweb">
        <team>blue-team</team>
        <material>web</material>
    </all>
</filters>
<regions>
</regions>
<itemremove>
    <item>iron sword</item>
    <item>bow</item>
    <item>iron pickaxe</item>
    <item>iron axe</item>
    <item>golden carrot</item>
    <item>wood</item>
    <item>arrow</item>
</itemremove>
<wools team="yellow">
    <wool color="light blue" location="1.5,10,81.5">
        <monument>
            <block>485.5,18,372.5</block>
        </monument>
    </wool>
</wools>
<wools team="blue">
    <wool color="yellow" location="1.5,10,-79.5">
        <monument>
            <block>313.5,18,291.5</block>
        </monument>
    </wool>
</wools>
<timelock>on</timelock>
<maxbuildheight>31</maxbuildheight>
</map>
