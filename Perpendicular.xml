<filters>
    <filter name="only-blue">
        <team>blue</team>
    </filter>
    <filter name="only-red">
        <team>red</team>
    </filter>
    <filter name="deny-fire">
        <deny>
            <block>fire</block>
        </deny>
    </filter>
    <filter name="deny-void">
        <deny>
            <void/>
        </deny>
    </filter>
    <filter name="deny-some">
        <not>
            <block>155:1</block>
            <block>95:15</block>
            <block>95:0</block>
            <block>159:9</block>
        </not>
    </filter>
    <filter name="deny-tnt-damage">
        <not>
            <all>
                <cause>explosion</cause>
                <block>tnt</block>
            </all>
        </not>
    </filter>
    <filter name="deny-tnt-damage-world">
        <not>
            <all>
                <cause>explosion</cause>
                <block>tnt</block>
                <any>
                    <block>155:0</block>
                    <block>155:1</block>
                    <block>155:2</block>
                    <block>95:15</block>
                    <block>95:0</block>
                    <block>159:9</block>
                </any>
            </all>
        </not>
    </filter>
    <filter name="deny-fireball-damage">
        <not>
            <all>
                <cause>explosion</cause>
                <item>fireball</item>
            </all>
        </not>
    </filter>
    <filter name="deny-fireball-damage-world">
        <not>
            <all>
                <cause>explosion</cause>
                <item>fireball</item>
                <any>
                    <block>155:0</block>
                    <block>155:1</block>
                    <block>155:2</block>
                    <block>95:15</block>
                    <block>95:0</block>
                    <block>159:9</block>
                </any>
            </all>
        </not>
    </filter>
</filters>
