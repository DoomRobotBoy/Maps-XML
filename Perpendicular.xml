        <item slot="22" damage="9" amount="64">stained clay</item>
        <item slot="5">water bucket</item>
        <item slot="32">water bucket</item>
        <item slot="23">water bucket</item>
        <item slot="6" amount="64">vine</item>
        <item slot="33" amount="16">ladder</item>
        <item slot="24" amount="64">step</item>
        <item slot="7" amount="16">redstone block</item>
        <item slot="34" amount="16">stone button</item>
        <item slot="25" amount="16">stone plate</item>
        <item slot="8" amount="64">tnt</item>
        <item slot="35" amount="16">nether fence</item>
        <helmet damage="-3000" enchantment="protection projectile:1">iron helmet</helmet>
        <potion duration="oo" amplifier="2">speed</potion>
    </kit>
    <kit name="red-bridger" parents="bridger">
        <chestplate damage="-3000" color="993333" enchantment="protection explosions:2">leather chestplate</chestplate>
        <leggings damage="-3000" color="993333" enchantment="protection projectile:1">leather leggings</leggings>
        <boots damage="-3000" color="993333">leather boots</boots>
    </kit>
    <kit name="blue-bridger" parents="bridger">
        <chestplate damage="-3000" color="334CB2" enchantment="protection explosions:2">leather chestplate</chestplate>
        <leggings damage="-3000" color="334CB2" enchantment="protection projectile:1">leather leggings</leggings>
        <boots damage="-3000" color="334CB2">leather boots</boots>
    </kit>
    <kit name="tank" parents="classes">
        
    </kit>
</kits>

<killreward>
    <filter>
        <class>Cannoneer</class>
    </filter>
    <item amount="8">arrow</item>
    <item>dispenser</item>
</killreward>
<killreward>
    <filter>
        <class>Cannoneer</class>
        <kill-streak count="3" repeat="true"/>
    </filter>
    <item amount="1">dispenser</item>
</killreward>
<killreward>
    <filter>
        <class>Cannoneer</class>
        <kill-streak count="5" repeat="true"/>
    </filter>
    <item amount="16" grenade="true" grenade-power="1" grenade-destroy="true" name="`eGrenade">snow ball</item>
</killreward>
<killreward>
    <filter>
        <class>Bridger</class>
        <class>Kamikaze</class>
        <kill-streak count="3" repeat="true"/>
    </filter>
    <item amount="16" grenade="true" grenade-power="1" grenade-destroy="true" name="`eGrenade">snow ball</item>
</killreward>
<killreward>
    <filter>
        <class>Bridger</class>
    </filter>
    <item amount="6">arrow</item>
</killreward>



<filters>
    <filter name="only-blue">
        <team>blue</team>
    </filter>
    <filter name="only-red">
        <team>red</team>
    </filter>
    <filter name="deny-fire">
        <deny>
            <block>fire</block>
        </deny>
    </filter>
    <filter name="deny-void">
        <deny>
            <void/>
        </deny>
    </filter>
    <filter name="deny-some">
        <not>
            <block>155</block>
            <block>95</block>
            <block>160</block>
            <block>44:7</block>
            <block>159:12</block>
            <block>159:15</block>
            <block>156</block>
            <block>20</block>
            <block>beacon</block>
            <block>22</block>
            <block>87</block>
        </not>
    </filter>
</filters>
<regions>
<rectangle name="all" min="-oo,-oo" max="oo,oo"/>
<cuboid name="above-map" min="-oo,33,-oo" max="oo,oo,oo"/>
<union name="spawns">
<cuboid name="blue-spawn" min="-27,3,70" max="28,3,84"/>
<cuboid name="red-spawn" min="28,3,-69" max="-27,3,-83"/>
</union>
<union name="bases">
<cuboid name="blue-base" min="-50,0,73" max="51,32,0"/>
<cuboid name="red-base" min="-50,0,-72" max="51,32,0"/>
</union>
<!--  Applied  -->
<apply block="deny-all" message="`eYou may not edit outside the main area!">
<negative>
<region name="bases"/>
</negative>
</apply>
<apply block-place="deny-void" message="`eYou may not bridge to the enemy side!">
<region name="bases"/>
</apply>
<apply block-place="deny-fire" message="`eYou may not burn down the bases!">
<region name="bases"/>
</apply>
<apply block-break="deny-some">
<region name="all"/>
</apply>
<apply enter="only-blue" message="`eYou may not enter the enemy's spawn area!">
<region name="blue-spawn"/>
</apply>
<apply enter="only-red" message="`eYou may not enter the enemy's spawn area!">
<region name="red-spawn"/>
</apply>
<apply enter="deny-all" message="`eYou may not go onto the roof">
<region name="above-map"/>
</apply>
</regions>
