<map proto="1.4.0" game="Blitz">
<name>Hell's Gate</name>
<version>1.0.0</version>
<objective>Eliminate the enemy team before time runs out!</objective>
<authors>
    <author uuid="dff980ed-9f69-491e-b79c-30c21350baf2"/> <!--   DoomRobotBoy   -->
</authors>
<teams>
    <team id="red" color="dark red" max="10>Red</team>
    <team id="blue" color="blue" max="10"">Blue</team>
</teams>
<spawns>
    <spawn team="red" kit="red-kit">
        <regions>
            <point yaw="-90">115.5,2,358.5</point>
        </regions>
    </spawn>
    <spawn team="bluet kit="blue-kit">
        <regions>
            <point yaw="90">195.5,2,358.5</point>
        </regions>
    </spawn>
    <default kit="all">
        <regions>
            <point yaw="-90">142.5,4,358.5</point>
        </regions>
    </default>
</spawns>
<kits>
    <kit id="all">
        <potion duration="oo">night vision</potion>
    </kit>
    <kit id="spawn" parents="all">
        <item slot="0" unbreakable="true">stone sword</item>
        <item slot="1" unbreakable="true">bow</item>
        <item slot="3" amount="64">golden carrot</item>
        <item slot="8" amount="20">arrow</item>
        <leggings locked="true" unbreakable="true">iron leggings</leggings>
    </kit>
    <kit id="red-kit" parents="spawn">
        <helmet color="993333" locked="true" unbreakable="true">leather helmet</helmet>
        <chestplate color="993333" locked="true" unbreakable="true">leather chestplate</chestplate>
        <boots color="993333" locked="true" unbreakable="true">leather boots</boots>
    </kit>
    <kit id="blue-kit" parents="spawn">
        <helmet color="334CB2" locked="true" unbreakable="true">leather helmet</helmet>
        <chestplate color="334CB2" locked="true" unbreakable="true">leather chestplate</chestplate>
        <boots color="334CB2" locked="true" unbreakable="true">leather boots</boots>
    </kit>
</kits>
<regions>
    <apply block="never">
        <region>
            <everywhere/>
        </region>
    </apply>
</regions>
<time>8m</time>
<blitz>
    <lives>1</lives>
</blitz>
<include src="blitz-global.xml"/>
<itemremove>
    <item>stone sword</item>
    <item>bow</item>
    <item>golden carrot</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>iron leggings</item>
    <item>leather boots</item>
</itemremove>
<gamerules>
    <naturalRegeneration>false</naturalRegeneration>
</gamerules>
<timelock>on</timelock>
</map>
