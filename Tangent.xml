<map proto="1.3.6">
<name>Tangent</name>
<version>1.0.0</version>
<objective>Be the first team to reach the score limit, or to hold the most points when time runs out.</objective>
<authors>
    <author>Diamyx</author>
</authors>
<contributors>
    <contributor contribution="XML">DoomRobotBoy</contributor>
</contributors>
<rules>
    <rule>Fall damage is disabled!</rule>
</rules>
<teams>
    <team color="red" max="16" max-overfill="20">Red</team>
    <team color="blue" max="16" max-overfill="20">Blue</team>
</teams>
<spawns>
<spawn team="red" kit="red" yaw="0">
    <regions><cuboid min="28,3,-78" max="-27,3,-75"/></regions>
</spawn>
<default kit="obs" yaw="90">
<point>-67.5,51,0.5</point>
</default>
</spawns>
<kits>
<kit name="obs">
<potion duration="oo" ambient="true">night vision</potion>
</kit>
<kit name="teams">
<item slot="0" damage="-3000">stone sword</item>
<item slot="1" damage="-3000" enchantment="arrow infinite">bow</item>
<item slot="2" damage="-3000">diamond pickaxe</item>
<item slot="4" amount="16">redstone block</item>
<item slot="31" amount="64">redstone</item>
<item slot="5" amount="64">tnt</item>
<item slot="32" amount="64">tnt</item>
<item slot="23" amount="64">tnt</item>
<item slot="6">water bucket</item>
<item slot="33">water bucket</item>
<item slot="24">water bucket</item>
<item slot="15" amount="8">trap door</item>
<item slot="26" amount="32">step</item>
<item slot="8" amount="64">redstone</item>
<item slot="34" amount="16">nether fence</item>
<item slot="35" amount="64">ladder</item>
<item slot="25" amount="4">diode</item>
<item slot="7" amount="16">stone button</item>
<item slot="16" amount="16">lever</item>
<item slot="17" amount="16">stone plate</item>
<item slot="29" damage="-3000" name="`6TNT Defuser" lore="`7Right click to defuse team griefer's TNT|`7Does not work in `9water`7.|`7Use `a/report `7to alert the staff!">shears</item>
<item slot="28">arrow</item>
<item slot="9" amount="8" damage="9" name="`aRepair Block" lore="`7Place this over your base to repair it!">stained clay</item>
<chestplate damage="-3000" enchantment="protection explosions:4;water worker:3">chainmail chestplate</chestplate>
<potion duration="oo" ambient="true">fast digging</potion>
<potion duration="oo" ambient="true">water breathing</potion>
<potion duration="20" ambient="true">heal</potion>
<potion duration="oo" ambient="true">damage resistance</potion>
</kit>
<kit name="blue" parents="teams">
<helmet damage="11">stained clay</helmet>
<leggings damage="-3000" color="0000FF">leather leggings</leggings>
<boots damage="-3000" color="0000FF">leather boots</boots>
<item slot="3" amount="64" damage="11">stained clay</item>
<item slot="30" amount="64" damage="11">stained clay</item>
<item slot="21" amount="64" damage="11">stained clay</item>
</kit>
<kit name="red" parents="teams">
<helmet damage="14">stained clay</helmet>
<leggings damage="-3000" color="FF0000">leather leggings</leggings>
<boots damage="-3000" color="FF0000">leather boots</boots>
<item slot="3" amount="64" damage="14">stained clay</item>
<item slot="30" amount="64" damage="14">stained clay</item>
<item slot="21" amount="64" damage="14">stained clay</item>
</kit>
</kits>
<filters>
<filter name="only-blue">
<team>blue</team>
</filter>
<filter name="only-red">
<team>red</team>
</filter>
<filter name="deny-fire">
<deny>
<block>fire</block>
</deny>
</filter>
<filter name="deny-void">
<deny>
<void/>
</deny>
</filter>
<filter name="deny-some">
<not>
<block>155</block>
<block>95</block>
<block>160</block>
<block>44:7</block>
<block>159:12</block>
<block>159:15</block>
<block>156</block>
<block>20</block>
<block>beacon</block>
<block>22</block>
<block>87</block>
</not>
</filter>
</filters>
<regions>
<rectangle name="all" min="-oo,-oo" max="oo,oo"/>
<cuboid name="above-map" min="-oo,33,-oo" max="oo,oo,oo"/>
<union name="spawns">
<cuboid name="blue-spawn" min="-27,3,70" max="28,3,84"/>
<cuboid name="red-spawn" min="28,3,-69" max="-27,3,-83"/>
</union>
<union name="bases">
<cuboid name="blue-base" min="-50,0,73" max="51,32,0"/>
<cuboid name="red-base" min="-50,0,-72" max="51,32,0"/>
</union>
<!--  Applied  -->
<apply block="deny-all" message="`eYou may not edit outside the main area!">
<negative>
<region name="bases"/>
</negative>
</apply>
<apply block-place="deny-void" message="`eYou may not bridge to the enemy side!">
<region name="bases"/>
</apply>
<apply block-place="deny-fire" message="`eYou may not burn down the bases!">
<region name="bases"/>
</apply>
<apply block-break="deny-some">
<region name="all"/>
</apply>
<apply enter="only-blue" message="`eYou may not enter the enemy's spawn area!">
<region name="blue-spawn"/>
</apply>
<apply enter="only-red" message="`eYou may not enter the enemy's spawn area!">
<region name="red-spawn"/>
</apply>
<apply enter="deny-all" message="`eYou may not go onto the roof">
<region name="above-map"/>
</apply>
</regions>
<itemremove>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>stone sword</item>
    <item>diamond pickaxe</item>
    <item>bow</item>
    <item>flint and steel</item>
    <item>arrow</item>
    <item>step</item>
    <item>chainmail chestplate</item>
    <item>shears</item>
    <item>rails</item>
    <item>ladder</item>
    <item>gold block</item>
    <item>obsidian</item>
    <item>stained clay</item>
    <item>bucket</item>
    <item>water bucket</item>
    <item>stone plate</item>
    <item>diode</item>
    <item>lever</item>
    <item>iron door</item>
    <item>fence</item>
    <item>wood</item>
    <item>tnt</item>
    <item>stone button</item>
    <item>redstone</item>
    <item>ladder</item>
    <item>log</item>
    <item>wood step</item>
    <item>gravel</item>
    <item>flint</item>
    <item>trap door</item>
    <item>fence gate</item>
</itemremove>
<killreward>
    <item amount="16">tnt</item>
    <item amount="16" damage="9" name="`aRepair Block" lore="`7Place this over your base to repair it!">stained clay</item>
</killreward>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<maxbuildheight>55</maxbuildheight>
</map>
