    <kit id="6-red" parents="red-kit">
        <potion duration="7" amplifier="5">damage resistance</potion>
    </kit>
    <kit id="9-red" parents="red-kit">
        <potion duration="10" amplifier="5">damage resistance</potion>
    </kit>
    <kit id="0-blue" parents="blue-kit">
        <potion duration="4" amplifier="5">damage resistance</potion>
    </kit>
    <kit id="6-blue" parents="blue-kit">
        <potion duration="7" amplifier="5">damage resistance</potion>
    </kit>
    <kit id="9-blue" parents="blue-kit">
