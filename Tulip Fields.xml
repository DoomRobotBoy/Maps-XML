<?xml version="1.0"?>
<map proto="1.3.6">
<name>Tulip Fields</name>
<version>1.0.0</version>
<objective>Get 100 kills first or the highest amount of kills after 10 minutes.</objective>
<authors>
    <author uuid="dff980ed-9f69-491e-b79c-30c21350baf2"/>
<!-- DoomRobotBoy -->
</authors>
<score> 
    <time>600</time>
    <limit>100</limit>
</score>
<teams>
    <team color="dark red" max="9" max-overfill="11">Red Team</team>
    <team color="blue" max="9" max-overfill="11">Blue Team</team>
    <team color="lime" max="9" max-overfill="11">Blue Team</team>
</teams>
<kits>
    <kit name="spawn" force="true">
        <item slot="0" attribute="generic.attackDamage:add:2">iron hoe</item>
        <item slot="1">bow</item>
        <item slot="2" amount="2">golden apple</item>
        <item slot="28" amount="32">egg</item>
        <chestplate damage="-3000">leather chestplate</chestplate>
        <leggings damage="-3000">chainmail leggings</leggings>
        <potion duration="10" amplifier="10">damage resistance</potion>
    </kit>
    <kit name="red" parents="spawn">
        <helmet damage="-3000" color="">leather helmet</helmet>
        <boots damage="-3000" color="">leather boots</boots>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet damage="-3000" color="">leather helmet</helmet>
        <boots damage="-3000" color="">leather boots</boots>
    </kit>
    <kit name="green" parents="spawn">
        <helmet damage="-3000" color="">leather helmet</helmet>
        <boots damage="-3000" color="">leather boots</boots>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="red">
        <regions yaw="">
            <cylinder base="285.5,13,-437.5" radius="1"/>
        </regions>
    </spawn>
    <spawn team="blue" kit="blue">
        <regions yaw="">
            <cylinder base="285.5,13,-437.5" radius="1"/>
        </regions>
    </spawn>
    <spawn team="green" kit="green">
        <regions yaw="">
            <cylinder base="54.5,34,-29.5" radius="1"/>
        </regions>
    </spawn>
    <default>
        <regions>
            <cylinder base="54.5,34,-29.5" radius="1"/>
        </regions>
    </default>
</spawns>
<modifybowprojectile>
    <projectile>egg</projectile>
</modifybowprojectile>
<regions>
    <apply block="only-cake" message="You may only place or destroy cakes.">
        <rectangle min="oo,oo" max="-oo,-oo"/>
    </apply>
</regions>
<killreward>
    <item>cake</item>
</killreward>
<killreward>
    <item amount="16">arrow</item>
</killreward>
<killreward>
    <filter>
        <kill-streak count="3"/>
    </filter>
    <kit force="true" potion-particles="false">
        <potion duration="oo" amplifier="2">speed</potion>
    </kit>
</killreward>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>diamond helmet</item>
    <item>diamond chestplate</item>
    <item>diamond leggings</item>
    <item>diamond boots</item>
    <item>cake</item>
    <item>golden apple</item>
    <item>cooked beef</item>
    <item>bow</item>
</itemremove>
<timelock>on</timelock>
</map>
