<?xml version="1.0"?>
<map proto="1.3.6">
<name>Shrine</name>
<version>1.0.0</version>
<objective>Capture the enemy flag and control the center flag to reach the score limit first.</objective>
<authors>
    <author uuid="dff980ed-9f69-491e-b79c-30c21350baf2" contribution="Main Author"/>
<!-- DoomRobotBoy -->
    <author uuid="557a98be-4ec0-4ac1-a2c4-73eee6c21a5e" contribution="Aesthetics, Roof"/>
<!-- Linehodres -->
    <author uuid="dd0aa4b8-f7d7-4e0e-9d13-e5f220f86a7e" contribution="Aesthetics, Roof"/>
<!-- lordblox -->
    <author uuid="256194af-149c-4216-8606-65418aa463ed" contribution="Aesthetics, Roof"/>
<!-- Manucide -->
</authors>
<contributors>
    <contributor uuid="03236737-c1f8-429c-83df-24b537365f5a" contribution="Moral Support"/>
<!-- Guicotsch -->
    <contributor uuid="c2848337-b526-4db0-9130-fabd0a5c7cfb" contribution="MapDev Server Host"/>
<!-- Rasta_Jol -->
</contributors>
<score>
    <limit>200</limit>
    <kills>2</kills>
    <deaths>1</deaths>
</score>
<teams>
    <team id="orange" color="gold" max="25" max-overfill="28">Orange Team</team>
    <team id="cyan" color="dark aqua" max="25" max-overfill="28">Cyan Team</team>
</teams>
<kits>
    <kit name="spawn">
        <item slot="0">stone sword</item>
        <item slot="1">bow</item>
        <item slot="2" amount="48">bread</item>
        <item slot="3" amount="1">golden apple</item>
        <item slot="28" amount="32">arrow</item>
        <leggings damage="-3000">chainmail leggings</leggings>
        <potion duration="5" amplifier="10">heal</potion>
        <potion duration="5" amplifier="10">damage resistance</potion>
    </kit>
    <kit name="orange" parents="spawn">
        <helmet color="D87F33" damage="-3000">leather helmet</helmet>
        <chestplate color="D87F33" damage="-3000">leather chestplate</chestplate>
        <boots color="D87F33" damage="-3000">leather boots</boots>
    </kit>
    <kit name="cyan" parents="spawn">
        <helmet color="4C7F99" damage="-3000">leather helmet</helmet>
        <chestplate color="4C7F99" damage="-3000">leather chestplate</chestplate>
        <boots color="4C7F99" damage="-3000">leather boots</boots>
    </kit>
</kits>
<filters>
    <filter name="only-orange">
        <team>Orange Team</team>
    </filter>
    <filter name="only-cyan">
        <team>Cyan Team</team>
    </filter>
</filters>
<regions>
    <rectangle name="map" min="oo,oo" max="-oo,-oo"/>
    <union name="orange-spawns">
        <cylinder base="164.5,11,125.5" radius="0"/>
        <cylinder base="252.5,11,125.5" radius="0"/>
    </union>
    <union name="cyan-spawns">
        <cylinder base="164.5,11,373.5" radius="0"/>
        <cylinder base="252.5,11,373.5" radius="0"/>
    </union>
    <union name="orange-nets">
    </union>
    <union name="cyan-nets">
    </union>
<!-- Applied -->
    <apply block="deny-all">
        <region name="map"/>
    </apply>
</regions>
<spawns>
    <spawn team="orange" kit="orange">
        <regions yaw="0">
            <region name="orange-spawns"/>
        </regions>
    </spawn>
    <spawn team="cyan" kit="cyan">
        <regions yaw="180">
            <region name="cyan-spawns"/>
        </regions>
    </spawn>
    <default>
        <regions yaw="-90">
            <cylinder base="208.5,50,249.5" radius="1" height="0"/>
        </regions>
    </default>
</spawns>
<flags>
    <post id="center-post">13,1,11</post>
    <post id="orange-post" owner="orange" yaw="90" points-rate="1" pickup-filter="only-orange">24,5,11</post>
    <post id="cyan-post" owner="cyan" yaw="90" points-rate="1" pickup-filter="only-cyan">24,5,11</post>
    <flag name="Flag" color="white" shared="true" post="center-post" points-rate="0.5">
        <net owner="orange" region="orange-nets" post="orange-post" capture-filter="only-orange"/>
        <net owner="cyan" region="cyan-nets" post="cyan-post" capture-filter="only-cyan"/>
    </flag>
    <flag id="orange-flag" name="Orange Flag" color="gold">
        <post pickup-filter="only-orange">2,5,11</post>
        <net points="1" region="orange-nets">
            <capture-filter>
                <flag-returned>orange-flag</flag-returned>
            </capture-filter>
        </net>
    </flag>
    <flag id="cyan-flag" name="Cyan Flag" color="dark aqua">
        <post pickup-filter="only-cyan">24,5,11</post>
        <net points="1" region="cyan-nets">
            <capture-filter>
                <flag-returned>cyan-flag</flag-returned>
            </capture-filter>
        </net>
    </flag>
</flags>
<killreward>
    <item amount="10">arrow</item>
    <item damage="8193" potions="regeneration:140:0:false;healing:1:0:false">potion</item>
</killreward>
<killreward>
    <filter>
        <kill-streak count="3" repeat="true"/>
    </filter>
    <item>golden apple</item>
</killreward>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>bread</item>
    <item>golden apple</item>
    <item>potion</item>
    <item>glass bottle</item>
</itemremove>
</map>
