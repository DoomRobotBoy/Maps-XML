<map proto="1.3.5">
<name>DK Crossovers DTE</name>
<version>1.2.0</version>
<objective>Destroy 70% of the enemy's side!</objective>
<authors>
    <author>DoomRobotBoy</author>
</authors>
<contributors>
    <contributor contribution="XML">orky48</contributor>
</contributors>
<teams>
    <team color="blue" max="30">Blue Team</team>
    <team color="green" max="30">Green Team</team>
</teams>
<rules>
    <rule>Fall damage is disabled!</rule>
</rules>
<kits>
    <kit name="spawn">
        <item slot="0">iron sword</item>
        <item slot="1" enchantment="arrow damage:1">bow</item>
        <item slot="2" enchantment="dig speed:2">iron pickaxe</item>
        <item slot="3" amount="64">vine</item>
        <item slot="5" amount="16">stone button</item>
        <item slot="6">water bucket</item>
        <item slot="7" amount="16">redstone block</item>
        <item slot="8" amount="64">tnt</item>
        <item slot="9" amount="3">dispenser</item>
        <item slot="14" amount="64">wood step</item>
        <item slot="15">water bucket</item>
        <item slot="17" amount="64">tnt</item>
        <item slot="16" amount="16">diode</item>
        <item slot="19" amount="64">arrow</item>
        <item slot="21" amount="32">fence</item>
        <item slot="23" amount="16">lever</item>
        <item slot="25" amount="16">redstone torch on</item>
        <item slot="24">water bucket</item>
        <item slot="26" amount="64">tnt</item>
        <item slot="28" amount="64">arrow</item>
        <item slot="30" amount="16">ladder</item>
        <item slot="34" amount="64">redstone</item>
        <item slot="35" amount="64">tnt</item>
        <item slot="32" amount="16">wood plate</item>
        <item slot="33">water bucket</item>
