        <potion duration="oo" amplifier="1">night vision</potion>
    </kit>
    <kit name="blue" parents="spawn">
        <item slot="4" amount="64" damage="11">stained clay</item>
        <item slot="31" amount="64" damage="11">stained clay</item>
        <item slot="22" amount="64" damage="11">stained clay</item>
        <helmet color="334CB2" damage="-3000">leather helmet</helmet>
        <chestplate enchantment="protection explosions:3" damage="-3000">chainmail chestplate</chestplate>
        <leggings damage="-3000">iron leggings</leggings>
        <boots color="334CB2" enchantment="protection fall:1" damage="-3000">leather boots</boots>
    </kit>
    <kit name="green" parents="spawn">
        <item slot="4" amount="64" damage="5">stained clay</item>
        <item slot="31" amount="64" damage="5">stained clay</item>
        <item slot="22" amount="64" damage="5">stained clay</item>
        <helmet color="667F33" damage="-3000">leather helmet</helmet>
        <chestplate enchantment="protection explosions:3">chainmail chestplate</chestplate>
        <leggings damage="-3000">iron leggings</leggings>
        <boots color="667F33" enchantment="protection fall:1" damage="-3000">leather boots</boots>
    </kit>
    <kit name="obs">
        <potion duration="oo" amplifier="1">night vision</potion>
    </kit>
</kits>
<spawns>
    <spawn team="blue" yaw="0" kit="blue">
        <cuboid min="989,114,444" max="998,114,446"/>
    </spawn>
    <spawn team="green" yaw="0" kit="green">
        <cuboid min="943,114,444" max="952,114,446"/>
    </spawn>
    <default yaw="0" kit="obs">
        <cuboid min="969,117,441" max="972,117,444"/>
    </default>
</spawns>
<filters>
    <filter name="deny-glass">
        <not>
            <block>glass</block>
        </not>
    </filter>
</filters>
<regions>
    <union name="jump-pads">
        <cuboid min="941,55,372" max="938,57,369"/>
        <cuboid min="941,31,436" max="938,33,433"/>
        <cuboid min="941,7,372" max="941,9,372"/>
        <cuboid min="999,55,425" max="1002,57,428"/>
        <cuboid min="999,31,361" max="1002,33,364"/>
        <cuboid min="999,7,425" max="1002,9,428"/>
    </union>
    <apply block-break="deny-glass" message="You may not edit the wall!">
        <cuboid min="968,0,439" max="973,120,359"/>
    </apply>
    <apply block="deny-blocks" message="Bridging is disabled in this area!">
        <cuboid min="944,117,439" max="996,59,359"/>
    </apply>
    <apply block="deny-blocks" message="You may not modify spawn!">
        <rectangle min="939,440" max="1008,454"/>
    </apply>
    <apply block="deny-all">
        <negative>
            <cuboid min="934,0,439" max="1007,119,359"/>
        </negative>
    </apply>
<!--  green jump pads - top to bottom  -->
    <apply velocity="@0.0,2.4,0.0" block="deny-all">
        <region name="jump-pads"/>
    </apply>
</regions>
<renewables>
    <renewable rate="6.5" grow="true" particles="false" sound="false">
        <region>
            <cuboid min="972,0,359" max="1007,119,440"/>
            <cuboid min="934,0,358" max="942,117,439"/>
        </region>
        <renew>stained clay:5</renew>
        <renew>stained clay:11</renew>
        <renew>stained clay:14</renew>
        <replace>stained clay:5</replace>
        <replace>stained clay:11</replace>
        <replace>stained clay:14</replace>
        <replace>air</replace>
    </renewable>
</renewables>
<destroyables>
    <destroyable name="Blue Side" owner="blue" completion="70%" show-progress="true" materials="stained clay:1;stained clay:3;stained clay:7;stained clay:8;stained clay:11;stained clay:12;stained clay:14;gold block;lapis block;redstone block;wood;wood:3;stained glass:4;wool;wool:7;wool:8;wool:14;stained clay:15">
        <cuboid min="972,0,359" max="1007,119,440"/>
    </destroyable>
    <destroyable name="Green Side" owner="green" completion="70%" show-progress="true" materials="stained clay:1;stained clay:3;stained clay:5;stained clay:7;stained clay:8;stained clay:12;stained clay:14;gold block;lapis block;redstone block;wood;wood:3;stained glass:4;wool;wool:7;wool:8;wool:14;stained clay:15">
        <cuboid min="934,0,358" max="942,117,439"/>
    </destroyable>
    <destroyable name="wall" owner="green" materials="glass;wool:4;stained clay:12;wood:1;gold block" completion="0" show="false" mode-changes="true">
        <cuboid min="968,0,439" max="973,120,359"/>
    </destroyable>
</destroyables>
<modes boss-bar="false">
    <mode after="1s" material="glass" name="`6The wall will fall down in `43 minutes`6!"/>
    <mode after="1m" material="glass" name="`6The wall will fall down in `42 minutes`6!"/>
    <mode after="2m" material="glass" name="`6The wall will fall down in `41 minute`6!"/>
    <mode after="150s" material="glass" name="`6The wall will fall down in `430 seconds`6!"/>
    <mode after="160s" material="glass" name="`6The wall will fall down in `420 seconds`6!"/>
    <mode after="170s" material="glass" name="`6The wall will fall down in `410 seconds`6!"/>
    <mode after="3m" material="air" name="`6The wall has been `6`lremoved!"/>
</modes>
<broadcasts>
    <tip after="10s">`7The cobweb areas mark where you are unable to bridge!</tip>
</broadcasts>
<portals>
<!--  Blue's portals  -->
