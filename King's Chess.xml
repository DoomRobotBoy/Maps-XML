<?xml version="1.0"?>
<map proto="1.4.0">
<name>King's Chess</name>
<version>1.0.0</version>
<objective>Reach the 1500 score limit, by capturing and keeping control of hills, before your enemy does!</objective>
<authors>
    <author uuid="dff980ed-9f69-491e-b79c-30c21350baf2"/>
<!-- DoomRobotBoy -->
</authors>
<teams>
    <team id="orange" color="gold" max="18" overfill="20">Orange Team</team>
    <team id="magenta" color="light purple" max="18" overfill="20">Magenta Team</team>
</teams>
<kits>
    <kit id="spawn">
        <item slot="0" damage="-3000">stone sword</item>
        <item slot="1" damage="-3000">bow</item>
        <item slot="28" amount="48">arrow</item>
        <item slot="2" amount="64">golden carrot</item>
        <item slot="3" damage="8261">potion</item>
        <chestplate damage="-3000">iron chestplate</chestplate>
        <potion duration="5">heal</potion>
        <potion duration="5" amplifier="10">damage resistance</potion>
    </kit>
    <kit id="o-kit" parents="spawn">
        <helmet color="667F33" damage="-3000">leather helmet</helmet>
        <leggings color="667F33" damage="-3000">leather leggings</leggings>
        <boots color="667F33" damage="-3000">leather boots</boots>
    </kit>
    <kit id="m-kit" parents="spawn">
        <helmet color="E5E533" damage="-3000">leather helmet</helmet>
        <leggings color="E5E533" damage="-3000">leather leggings</leggings>
        <boots color="E5E533" damage="-3000">leather boots</boots>
    </kit>
</kits>
<filters>
    <any id="only-some">
        <material>ladder</material>
        <material>vine</material>
        <material>leaves:2</material>
    </any>
    <any id="orange-southwest">
        <not>
            <all>
                <objective team="orange">s-hill</objective>
                <objective team="orange">e-hill</objective>
                <objective team="orange">n-hill</objective>
                <objective team="orange">w-hill</objective>
            </all>
        </not>
        <objective team="orange">s-hill</objective>
        <all>
            <objective team="orange">s-hill</objective>
            <objective team="orange">w-hill</objective>
        </all>
        <all>
            <objective team="orange">s-hill</objective>
            <objective team="orange">w-hill</objective>
            <objective team="orange">e-hill</objective>
        </all>
        <all>
            <objective team="orange">s-hill</objective>
            <objective team="orange">w-hill</objective>
            <objective team="orange">n-hill</objective>
        </all>
    </any>
    <any id="magenta-northeast">
        <not>
            <all>
                <objective team="magenta">s-hill</objective>
                <objective team="magenta">e-hill</objective>
                <objective team="magenta">n-hill</objective>
                <objective team="magenta">w-hill</objective>
            </all>
        </not>
        <objective team="magenta">n-hill</objective>
        <all>
            <objective team="magenta">n-hill</objective>
            <objective team="magenta">e-hill</objective>
        </all>
        <all>
            <objective team="magenta">n-hill</objective>
            <objective team="magenta">e-hill</objective>
            <objective team="magenta">w-hill</objective>
        </all>
        <all>
            <objective team="magenta">n-hill</objective>
            <objective team="magenta">e-hill</objective>
            <objective team="magenta">s-hill</objective>
        </all>
    </any>
</filters>
<regions>
    <apply block="only-some">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
    <apply enter="only-green">
        <cuboid min="145,25,-199" max="149,26,-200"/>
    </apply>
    <apply enter="only-yellow">
        <cuboid min="286,25,-250" max="282,26,-249"/>
    </apply>
</regions>
<spawns>
    <spawn team="orange" kit="o-kit" filter="orange-southwest">
        <regions yaw="90"/>
            <cylinder base="279.5,38,-231.5" radius="1"/>
        </regions>
    </spawn>
    <spawn team="magenta" kit="m-kit" filter="magenta-northeast">
        <regions yaw="-90"/>
            <cylinder base="151.5,38,-217.5" radius="1"/>
        </regions>
    </spawn>
    <default>
        <regions yaw="180"/>
            <cylinder base="215.5,53,-292.5" radius="2"/>
        </regions>
    </default>
</spawns>
<king>
    <hills>
        <hill id="Green Hill" capture-players="lead" capture-time="10s" ratio="2" incremental="true">
            <capture><cylinder base="275.5,38,-191.5" radius="2" height="3"/></capture>
            <progress><cylinder base="275.5,37,-191.5" radius="2"/></progress>
            <captured><cylinder base="275.5,37,-191.5" radius="2"/></captured>
        </hill>
    </hills>
</king>
<itemremove>
    <item>stone sword</item>
    <item>bow</item>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>iron chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>golden carrot</item>
    <item>potion</item>
    <item>glass bottle</item>
</itemremove>
<score>
    <limit>1500</limit>
    <kills>1</kills>
</score>
<killreward>
    <item damage="8261">potion</item>
</killreward>
</map>
