<?xml version="1.0"?>
<map proto="1.3.6">
<name>Mutiny</name>
<version>1.0.0</version>
<objective>Leak lava from the enemy's obsidian core into the void.</objective>
<authors>
    <author>Avaleer</author>
    <author>Sucitar</author>
</authors>
<contributors>
    <contributor contribution="XML">DoomRobotBoy</contributor>
</contributors>
<teams>
    <team color="dark red" max="18" max-overfill="22">Red Team</team>
    <team color="blue" max="18" max-overfill="22">Blue Team</team>
</teams>
<kits>
    <kit name="spawn" force="true">
        <item slot="0">stone sword</item>
        <item slot="1">bow</item>
        <item slot="28" amount="64">arrow</item>
        <item slot="2" amount="64">wood:2</item>
        <item slot="2" amount="64">wood:2</item>
        <item slot="2" amount="64">wood:2</item>
        <item slot="2" amount="64">wood:2</item><item slot="2" amount="64">wood:2</item>
    </kit>
</kits>
<filters>
    <filter name="void">
        <void/>
    </filter>
</filters>
<regions>
    <apply block="deny-all">
        <union name="spawns-protection">
            <rectangle name="red-spawn-protect" min="" max=""/>
            <rectangle name="blue-spawn-protect" min="" max=""/>
        </union>
    </apply>
<!-- Void filter must go last, since it overrides all regions under it -->
    <apply block="void" message="You may not modify the void area!">
        <region min="oo,oo" max="-oo,-oo"/>
    </apply>
</regions>
