<map proto="1.4.0">
<name>Valle Escondido</name>
<version>1.0.0</version>
<objective>Destroy the enemy monuments.</objective>
<authors>
	<author uuid="2d812e55-9386-4988-8387-4b8b868bf7c0"/>
	<!--    alaann    -->
	<author uuid="69b7966b-d00f-45a1-b399-28b910539846"/>
	<!--    Santii98    -->
</authors>
<contributors>
    <contributor uuid="0b8d2617-0b3f-4568-a6cd-2b56dae96a5b"/>
	<!--    zoodi   -->
	<contributor uuid="f926e3b1-ce9c-4269-a5e8-e339d5056480"/>
	<!--    Matilaina   -->
	<contributor contribution="XML" uuid="dff980ed-9f69-491e-b79c-30c21350baf2"/>
	<!--    DoomyBoy   -->
</contributors>
<teams>
	<team id="blue-team" color="blue" max="5">Blue</team>
	<team id="red-team" color="dark red" max="5">Red</team>
</teams>
<kits>
	<kit id="spawn">
		<item slot="0" unbreakable="true" material="stone sword"/>
		<item slot="1" unbreakable="true" enchantment="arrow infinite:1" material="bow"/>
		<item slot="28" material="arrow"/>
		<item slot="2" unbreakable="true" material="stone pickaxe"/>
		<item slot="3" amount="16" material="cooked rabbit"/>
		<item slot="4" amount="64" material="wood"/>
		<item slot="5" amount="64" material="wood"/>
		<item slot="6" amount="32" material="glass"/>
	</kit>
</kits>
<filters>
    <team id="only-blue">blue</team>
    <team id="only-red">red</team>
    <not id="no-void">
        <void/>
    </not>
</filters>
<regions>
	<below y="27" id="below-27"/>
	<above y="1" id="above-1"/>
	<above y="2" id="above-2"/>
	<union id="red-lanes">
		<intersect id="red-middle">
			<rectangle min="50,9" max="29,23"/>
			<region id="above-1"/>
		</intersect>
		<rectangle id="red-front" min="49,23" max="27,40"/>
		<rectangle id="red-water" min="41,40" max="28,77"/>
		<union id="red-back"/>
	</union>
	<rectangle id="center-water" min="42,60" max="-13,76"/>
	<union id="blue-lanes">
		<intersect id="blue-middle">
			<rectangle min="-1,23" max="-22,9"/>
			<region id="above-1"/>
		</intersect>
		<rectangle id="blue-front" min="-21,23" max="1,40"/>
		<rectangle id="blue-water" min="-13,40" max="0,77"/>
		<union id="blue-back"/>
	</union>
</regions>
<destroyables>
    <destroyables name="Coal Monument">
        <destroyable owner="red" materials="coal block">
            <region>
                <cuboid min="40,6,66" max="37,10,63"/>
            </region>
        </destroyable>
        <destroyable owner="blue" materials="coal block">
            <region>
                <cuboid min="-12,6,66" max="-9,10,63"/>
            </region>
        </destroyable>
    </destroyables>
    <destroyables name="Emerald Monument">
        <destroyable owner="red" materials="emerald block">
            <region>
                <cuboid min="45,7,17" max="48,11,14"/>
            </region>
        </destroyable>
        <destroyable owner="blue" materials="emerald block">
            <region>
                <cuboid min="-17,7,17" max="-20,11,14"/>
            </region>
        </destroyable>
    </destroyables>
</destroyables>
<spawns>
	<spawn team="red" kit="spawn">
        <regions>
            <point yaw="45">71.5,59,-21.5</point>
        </regions>
    </spawn>
    <spawn team="blue" kit="spawn">
        <regions>
            <point yaw="-45">-43.5,59,-21.5</point>
        </regions>
    </spawn>
    <default>
        <regions>
            <point yaw="180">14.5,44,-14.5</point>
        </regions>
    </default>
</spawns>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
    <tool>stone pickaxe</tool>
    <tool>arrow</tool>
</toolrepair>
<itemremove>
	<item>dispenser</item>
	<item>emerald block</item>
	<item>coal block</item>
</itemremove>
<maxbuildheight>26</maxbuildheight>
<time result="objectives">30m</time>
</map>
