<map proto="1.4.0">
<name>Summer Cottage TE</name>
<version>1.0.0</version>
<objective>Destroy the enemy monuments.</objective>
<authors>
	<author uuid="2a2c4c30-ecf6-4e4a-b5bc-be8cf033563e"/>
	<!--    felipu_feliu    -->
	<author uuid="396229db-3e91-415f-be44-8bacc9fcbc9f"/>
	<!--    CIPATO    -->
</authors>
<contributors>
	<contributor contribution="Help and XML" uuid="dff980ed-9f69-491e-b79c-30c21350baf2"/>
	<!--    DoomyBoy   -->
</contributors>
<respawn delay="3s"/>
<teams>
	<team id="blue-team" color="blue" max="5">Blue</team>
	<team id="red-team" color="dark red" max="5">Red</team>
</teams>
<kits>
	<kit id="spawn">
		<helmet unbreakable="true" material="leather helmet"/>
		<chestplate unbreakable="true" material="chainmail chestplate"/>
	</kit>
</kits>
<filters>
    <team id="only-blue">blue</team>
    <team id="only-red">red</team>
    <not id="no-void">
        <void/>
    </not>
</filters>
<regions>
	
</regions>
<destroyables>
    <destroyables name="East Monument">
        <destroyable owner="red" materials="obsidian;coal block">
            <region>
                <cuboid min="12,19,43" max="9,22,46"/>
            </region>
        </destroyable>
        <destroyable owner="blue" materials="obsidian;gold block">
            <region>
                <cuboid min="12,15,-26" max="15,18,-29"/>
            </region>
        </destroyable>
    </destroyables>
    <destroyables name="West Monument">
        <destroyable owner="red" materials="obsidian;gold block">
            <region>
                <cuboid min="-41,15,30" max="-44,18,33"/>
            </region>
        </destroyable>
        <destroyable owner="blue" materials="obsidian;coal block">
            <region>
                <cuboid min="-41,19,-39" max="-38,22,-42"/>
            </region>
        </destroyable>
    </destroyables>
</destroyables>
<spawns>
    <spawn team="blue" kit="spawn">
        <regions>
            <point yaw="0">-14.5,18,-54.5</point>
        </regions>
    </spawn>
    <spawn team="red" kit="spawn">
        <regions>
            <point yaw="180">-14.5,18,58.5</point>
        </regions>
    </spawn>
    <default>
        <regions yaw="-90" pitch="-25">
            <cuboid min="34,28,3" max="35,28,1"/>
        </regions>
    </default>
</spawns>
<itemremove>
	<item>stone sword</item>
	<item>bow</item>
	<item>iron pickaxe</item>
	<item>iron axe</item>
	<item>stone spade</item>
	<item>log</item>
	<item>log 2</item>
	<item>wood</item>
	<item>coal block</item>
	<item>gold block</item>
	<item>obsidian</item>
</itemremove>
<maxbuildheight>25</maxbuildheight>
<time result="objectives">35m</time>
</map>
