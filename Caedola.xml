<name>Caedola</name>
<version>1.0.0</version>
<objective>Be the first team to reach 500 points.</objective>
<authors>
    <author uuid="e3fdaea1-8a19-4378-8ad7-41cf63f5d141"/>
<!--  avaleer  -->
</authors>
<contributors>
    <contributor uuid="dff980ed-9f69-491e-b79c-30c21350baf2" contribution="XML"/>
<!--  DoomRobotBoy  -->
    <contributor uuid="fe3608b7-d105-4029-8800-34b3147065b6" contribution="Feedback"/>
<!--  rockymine  -->
</contributors>
<teams>
    <team id="red" color="dark red" max="20">Red Team</team>
    <team id="blue" color="blue" max="20">Blue Team</team>
</teams>
<kits>
    <kit name="spawn" force="true">
        <item slot="0" damage="-3000">stone sword</item>
        <item slot="1" damage="-3000" enchantment="arrow infite:1">bow</item>
        <item slot="28">arrow</item>
        <potion duration="5" amplifier="25">heal</potion>
        <potion duration="5" amplifier="25">damage resistance</potion>
    </kit>
    <kit name="red" parents="spawn">
        <chestplate color="993333" damage="-3000">leather chestplate</chestplate>
    </kit>
    <kit name="blue" parents="spawn">
        <chestplate color="334CB2" damage="-3000">leather chestplate</chestplate>
    </kit>
</kits>
<killreward>
    <item>iron ingot</item>
</killreward>
<spawns>
    <spawn team="red" kit="red">
        <regions yaw="180">
            <cylinder base="54.5,5,76.5" radius="1"/>
        </regions>
    </spawn>
    <spawn team="blue" kit="blue">
        <regions yaw="0">
            <cylinder base="54.5,5,24.5" radius="1"/>
        </regions>
    </spawn>
    <default>
        <regions>
            <cylinder base="54.5,5,50.5" radius="0"/>
        </regions>
    </default>
</spawns>
<filters>
    <filter name="only-some">
        <block>gold block</block>
        <block>glass</block>
        <block>web</block>
    </filter>
</filters>
<regions>
    <union name="gold-blocks">
        <block>70.5,6,50.5</block>
        <block>38.5,6,50.5</block>
    </union>
    <apply block="only-some">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<renewables>
    <renewable rate="0.3" particles="true" sound="true" avoid-entities="true">
        <region>
            <region name="gold-blocks"/>
        </region>
        <renew>gold block</renew>
    </renewable>
</renewables>
<blockdrops>
    <rule>
        <region>
            <region name="gold-blocks"/>
        </region>
        <filter>
            <block>gold block</block>
        </filter>
        <drops>
            <item>gold ingot</item>
        </drops>
    </rule>
</blockdrops>
<king>
    <hills capture-time="15s" neutral-state="true" points="5">
        <hill name="Alpha Hill">
            <capture>
                <cylinder base="22.5,5,50.5" radius="2" height="2"/>
            </capture>
            <progress>
                <cylinder base="22.5,4,50.5" radius="2" height="1"/>
            </progress>
            <captured>
                <cylinder base="22.5,4,50.5" radius="2" height="1"/>
            </captured>
        </hill>
        <hill name="Omega Hill">
            <capture>
                <cylinder base="86.5,5,50.5" radius="2" height="2"/>
            </capture>
            <progress>
                <cylinder base="86.5,4,50.5" radius="2" height="1"/>
            </progress>
            <captured>
                <cylinder base="86.5,4,50.5" radius="2" height="1"/>
            </captured>
        </hill>
    </hills>
</king>
<score>
    <time>15m</time>
    <limit>5000</limit>
</score>
<itemremove>
    <item>stone sword</item>
    <item>bow</item>
    <item>arrow</item>
    <item>leather chestplate</item>
    <item>potion</item>
    <item>web</item>
    <item>iron pickaxe</item>
</itemremove>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
<maxbuildheight>6</maxbuildheight>
</map>
