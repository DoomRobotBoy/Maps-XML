<?xml version="1.0"?>
<map proto="1.3.6">
<name>The Last Sunset</name>
<version>1.0.0</version>
<objective>Capture the enemy flag(s) 5 times!</objective>
<authors>
    <author uuid="dff980ed-9f69-491e-b79c-30c21350baf2"/>
<!-- DoomRobotBoy -->
</authors>
<teams>
    <team id="red" color="dark red" max="20" max-overfill="24">Red Team</team>
    <team id="blue" color="blue" max="20" max-overfill="24">Blue Team</team>
</teams>
<kits>
    <kit name="spawn">
        <item slot="0" damage="-3000">stone sword</item>
        <item slot="1" damage="-3000" enchantment="arrow infinite:1">bow</item>
        <item slot="2" damage="-3000">diamond pickaxe</item>
        <item slot="3">iron axe</item>
        <item slot="4">wood spade</item>
        <item slot="5" amount="32">bread</item>
        <item slot="6" amount="32">wood</item>
        <item slot="7" amount="16">glass</item>
        <item slot="8" amount="32">vines</item>
        <item slot="28" amount="1">arrow</item>
        <potion duration="3" amplifier="127">damage resistance</potion>
        <potion duration="oo">night vision</potion>
    </kit>
    <kit name="red" parents="spawn">
        <helmet color="993333" damage="-3000">leather helmet</helmet>
        <chestplate color="993333" enchantment="protection projectile:2" damage="-3000">leather chestplate</chestplate>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet color="334CB2" damage="-3000">leather helmet</helmet>
        <chestplate color="334CB2" enchantment="protection projectile:2" damage="-3000">leather chestplate</chestplate>
    </kit>
    <kit name="red-kit">
        <leggings enchantment="durability:3" locked="true">gold leggings</leggings>
        <boots enchantment="durability:3" locked="true">gold boots</boots>
    </kit>
    <kit name="blue-kit">
        <leggings enchantment="durability:3" locked="true">gold leggings</leggings>
        <boots enchantment="durability:3" locked="true">gold boots</boots>
    </kit>
    <kit name="red-no-kit">
        <item slot="0" damage="-3000">stone sword</item>
        <item slot="1" damage="-3000" enchantment="arrow infinite:1">bow</item>
        <item slot="2" damage="-3000">diamond pickaxe</item>
        <item slot="3">iron axe</item>
        <item slot="4">wood spade</item>
        <item slot="5" amount="32">bread</item>
        <item slot="6" amount="32">wood</item>
        <item slot="7" amount="16">glass</item>
        <item slot="8" amount="32">vines</item>
        <item slot="28" amount="1">arrow</item>
        <helmet color="993333" damage="-3000">leather helmet</helmet>
        <chestplate color="993333" enchantment="protection projectile:2" damage="-3000">leather chestplate</chestplate>
        <potion duration="oo">night vision</potion>
        <clear/>
    </kit>
    <kit name="blue-no-kit">
        <item slot="0" damage="-3000">stone sword</item>
        <item slot="1" damage="-3000" enchantment="arrow infinite:1">bow</item>
        <item slot="2" damage="-3000">diamond pickaxe</item>
        <item slot="3">iron axe</item>
        <item slot="4">wood spade</item>
        <item slot="5" amount="32">bread</item>
        <item slot="6" amount="32">wood</item>
        <item slot="7" amount="16">glass</item>
        <item slot="8" amount="32">vines</item>
        <item slot="28" amount="1">arrow</item>
        <helmet color="334CB2" damage="-3000">leather helmet</helmet>
        <chestplate color="334CB2" enchantment="protection projectile:2" damage="-3000">leather chestplate</chestplate>
        <potion duration="oo">night vision</potion>
        <clear/>
    </kit>
</kits>
<filters>
    <filter name="no-void">
        <not>
            <void/>
        </not>
    </filter>
    <filter name="only-red">
        <team>red</team>
    </filter>
    <filter name="only-blue">
        <team>blue</team>
    </filter>
</filters>
<regions>
    <rectangle name="map" min="oo,oo" max="-oo,-oo"/>
    <union name="spawn-areas">
        <rectangle name="blue-spawn" min="-838,-402" max="-818,-385"/>
        <rectangle name="red-spawn" min="-937,-385" max="-957,-385"/>
    </union>
    <union name="red-flag-posts">
        <block>-968.5,5,-347.5</block>
        <block>-968.5,5,-439.5</block>
    </union>
    <union name="blue-flag-posts">
        <block>-806.5,5,-347.5</block>
        <block>-806.5,5,-439.5</block>
    </union>
<!-- Applied -->
    <apply block="deny-all" message="You may not modify the spawn areas!">
        <region name="spawn-areas"/>
    </apply>
    <apply block="no-void" message="You may not modify the void area!">
        <region name="map"/>
    </apply>
</regions>
<spawns>
    <spawn team="red" kit="red">
        <regions yaw="-90">
            <cylinder base="-949.5,4,-393.5" radius="1"/>
        </regions>
    </spawn>
    <spawn team="blue" kit="blue">
        <regions yaw="90">
            <cylinder base="-825.5,4,-393.5" radius="1"/>
        </regions>
    </spawn>
    <default>
        <regions yaw="0">
            <cylinder base="-887.5,47,452.5" radius="1"/>
        </regions>
    </default>
</spawns>
<score>
    <limit>5</limit>
</score>
<flags>
    <flag id="south-red-flag" name="South Red Flag" color="dark red" shared="true" return-time="15s" pickup-kit="blue-kit" drop-kit="blue-no-kit">
        <post pickup-filter="only-blue">-968.5,5,-347.5</post>
        <net points="1" region="blue-flag-posts"/>
        <capture-filter>
            <any>
                <flag-returned>north-blue-flag</flag-returned>
                <flag-returned>south-blue-flag</flag-returned>
            </any>
        </capture-filter>
    </flag>
    <flag id="north-red-flag" name="North Red Flag" color="dark red" shared="true" return-time="15s" pickup-kit="blue-kit" drop-kit="blue-no-kit">
        <post pickup-filter="only-blue">-968.5,5,-439.5</post>
        <net points="1" region="blue-flag-posts"/>
        <capture-filter>
            <any>
                <flag-returned>north-blue-flag</flag-returned>
                <flag-returned>south-blue-flag</flag-returned>
            </any>
        </capture-filter>
    </flag>
    <flag id="south-blue-flag" name="South Blue Flag" color="blue" shared="true" return-time="15s" pickup-kit="red-kit" drop-kit="red-no-kit">
        <post pickup-filter="only-red">-806.5,5,-347.5</post>
        <net points="1" region="red-flag-posts"/>
        <capture-filter>
            <any>
                <flag-returned>north-red-flag</flag-returned>
                <flag-returned>south-red-flag</flag-returned>
            </any>
        </capture-filter>
    </flag>
    <flag id="north-blue-flag" name="North Blue Flag" color="blue" shared="true" return-time="15s" pickup-kit="red-kit" drop-kit="red-no-kit">
        <post pickup-filter="only-red">-806.5,5,-439.5</post>
        <net points="1" region="red-flag-posts"/>
        <capture-filter>
            <any>
                <flag-returned>north-red-flag</flag-returned>
                <flag-returned>south-red-flag</flag-returned>
            </any>
        </capture-filter>
    </flag>
</flags>
<toolrepair>
    <tool>stone sword</tool>
    <tool>diamond pickaxe</tool>
    <tool>bow</tool>
    <tool>stone axe</tool>
    <tool>wooden spade</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>arrow</item>
    <item>glass</item>
    <item>stained clay:11</item>
    <item>stained clay:14</item>
    <item>stained clay:15</item>
    <item>coal block</item>
</itemremove>
<maxbuildheight>20</maxbuildheight>
</map>
