<map proto="1.4.0">
<name>Grayscale</name>
<version>1.0.0</version>
<objective>Capture all 5 sequential control points or hold the most when the time runs out!</objective>
<authors>
    <author uuid="dff980ed-9f69-491e-b79c-30c21350baf2"/> <!-- DoomRobotBoy -->
</authors>
<contributors>
    <contributor uuid="2958ea4c-3501-404e-88cc-12e8632f68cc" contribution="Feedback and suggestions"/> <!-- NixaniaXi -->
</contributors>
<teams>
    <team id="red" color="dark red" max="10">Red</team>
    <team id="blue" color="blue" max="10">Blue</team>
</teams>
<kits>
    <kit id="spawn">
        <item slot="0">stone sword</item>
        <item slot="1">bow</item>
        <item slot="2" amount="32">cooked beef</item>
        <item slot="20">arrow</item>
        <leggings unbreakable="true">iron leggings</leggings>
    </kit>
    <kit id="red-kit" parents="spawn">
        <helmet color="cd0000" unbreakable="true">leather helmet</helmet>
        <chestplate color="cd0000" unbreakable="true">leather chestplate</chestplate>
        <boots color="cd0000" unbreakable="true">leather boots</boots>
    </kit>
    <kit id="blue-kit" parents="spawn">
        <helmet color="0066cc" unbreakable="true">leather helmet</helmet>
        <chestplate color="0066cc" unbreakable="true">leather chestplate</chestplate>
        <boots color="0066cc" unbreakable="true">leather boots</boots>
    </kit>
</kits>
<killreward>
    <item damage="8193" potions="regeneration:100:0:false">potion</item>
</killreward>
<itemremove>
    <item>stone sword</item>
    <item>bow</item>
    <item>cooked beef</item>
    <item>arrow</item>
    <item>potion</item>
    <item>glass bottle</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather boots</item>
    <item>iron leggings</item>
</itemremove>
<regions>
    <apply block="never" message="You can only break leaves!">
        <region>
            <everywhere/>
        </region>
    </apply>
</regions>
<spawns>
    <spawn team="red" kit="red-kit">
        <regions yaw="135">
            <cylinder base="-18.5,25,212.5" radius="1" height="0"/>
        </regions>
    </spawn>
    <spawn team="blue" kit="blue-kit">
        <regions yaw="45">
            <cylinder base="-18.5,25,40.5" radius="1" height="0"/>
        </regions>
    </spawn>
    <default>
        <regions yaw="90">
            <cylinder base="-60.5,32,126.5" radius=".5" height="0"/>
        </regions>
    </default>
</spawns>
<control-points incremental="true" neutral-state="false" show-progress="true">
    <control-point initial-owner="red" id="red-last" name="Red Courtyard" capture-time="10s">
        <capture>
            <cylinder base="-35.5,24,232.5" radius="6" height="2"/>
        </capture>
        <progress>
            <cylinder base="-35.5,23,232.5" radius="6" height="1"/>
        </progress>
        <captured>
            <cylinder base="-35.5,23,232.5" radius="6" height="1"/>
        </captured>
        <player-filter>
            <any>
                <objective>red-2nd</objective>
                <team>red</team>
            </any>
        </player-filter>
    </control-point>
    <control-point initial-owner="red" id="red-2nd" name="Red Aqueduct" capture-time="12s">
        <capture>
            <cylinder base="-96.5,22,187.5" radius="6" height="2"/>
        </capture>
        <progress>
            <cylinder base="-96.5,21,187.5" radius="6" height="2"/>
        </progress>
        <captured>
            <cylinder base="-96.5,21,187.5" radius="6" height="2"/>
        </captured>
        <player-filter>
            <any>
                <objective>red-last</objective>
                <all>
                    <objective>mid-dam</objective>
                    <objective>mid-river</objective>
                </all>
            </any>
        </player-filter>
    </control-point>
    <control-point id="mid-dam" name="Dam" capture-time="15s">
        <capture>
            <cylinder base="-48.5,24,126.5" radius="7" height="2"/>
        </capture>
        <progress>
            <cylinder base="-48.5,23,126.5" radius="7" height="1"/>
        </progress>
        <captured>
            <cylinder base="-48.5,23,126.5" radius="7" height="1"/>
        </captured>
        <player-filter>
            <any>
                <objective>red-2nd</objective>
                <objective>blue-2nd</objective>
            </any>
        </player-filter>
    </control-point>
    <control-point id="mid-river" name="River" capture-time="15s">
        <capture>
            <cylinder base="-150.5,18,126.5" radius="6" height="2"/>
        </capture>
        <progress>
            <cylinder base="-150.5,17,126.5" radius="6" height="1"/>
        </progress>
        <captured>
            <cylinder base="-150.5,17,126.5" radius="6" height="1"/>
        </captured>
        <player-filter>
            <any>
                <objective>red-2nd</objective>
                <objective>blue-2nd</objective>
            </any>
        </player-filter>
    </control-point>
    <control-point initial-owner="blue" id="blue-2nd" name="Blue Aqueduct" capture-time="12s">
        <capture>
            <cylinder base="-96.5,22,65.5" radius="6" height="2"/>
        </capture>
        <progress>
            <cylinder base="-96.5,21,65.5" radius="6" height="1"/>
        </progress>
        <captured>
            <cylinder base="-96.5,21,65.5" radius="6" height="1"/>
        </captured>
        <player-filter>
            <any>
                <all>
                    <objective>mid-dam</objective>
                    <objective>mid-river</objective>
                </all>
                <objective>blue-last</objective>
            </any>
        </player-filter>
    </control-point>
    <control-point initial-owner="blue" id="blue-last" name="Blue Courtyard" capture-time="10s">
        <capture>
            <cylinder base="-35.5,24,20.5" radius="6" height="2"/>
        </capture>
        <progress>
            <cylinder base="-35.5,23,20.5" radius="6" height="1"/>
        </progress>
        <captured>
            <cylinder base="-35.5,23,20.5" radius="6" height="1"/>
        </captured>
        <player-filter>
            <any>
                <objective>blue-2nd</objective>
                <team>blue</team>
            </any>
        </player-filter>
    </control-point>
</control-points>
<time result="objectives">12m</time>
</map>
